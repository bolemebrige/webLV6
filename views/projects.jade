extends layout
block content
  table
   each val in data
    tr
      td=val.id
      td=val.naziv
      td=val.opis
      td=val.cijena
      td=val.obavljeno
      td=val.start_date
      td=val.end_date

  form(action='/handleProjects', method='post')
    | ID :
    br
    input(type='text', name='id')
    br

 
    input(type='radio', value='Obriši', name="action")
    | Obriši
    br
    input(type='radio', value='Uredi', name="action")
    | Uredi
    br
    input(type="submit" value="submit")
br
